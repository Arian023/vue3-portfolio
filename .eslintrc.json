{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["vue", "prettier"],
  "rules": {
    "import/prefer-default-export": "off",
    "vue/multi-word-component-names": "off",
    "vue/no-multiple-template-root": "off"
  }
}
